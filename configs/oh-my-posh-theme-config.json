{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "background": "#c386f1",
                    "foreground": "#ffffff",
                    "leading_diamond": "ÓÇ∂",
                    "style": "diamond",
                    "template": " {{ .UserName }} ",
                    "trailing_diamond": "ÓÇ∞",
                    "type": "session"
                },
                {
                    "background": "#ff479c",
                    "foreground": "#ffffff",
                    "powerline_symbol": "ÓÇ∞",
                    "properties": {
                        "folder_separator_icon": " ÓÇ± ",
                        "home_icon": "~",
                        "style": "folder"
                    },
                    "style": "powerline",
                    "template": " Ó™É  {{ .Path }} ",
                    "type": "path"
                },
                {
                    "background": "#010409",
                    "background_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
                        "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
                        "{{ if gt .Behind 0 }}#B388FF{{ end }}"
                    ],
                    "foreground": "#193549",
                    "leading_diamond": "ÓÇ∂",
                    "powerline_symbol": "ÓÇ∞",
                    "properties": {
                        "branch_max_length": 25,
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "fetch_upstream_icon": true
                    },
                    "style": "powerline",
                    "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} ÔÅÑ {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} ÔÅÜ {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} Ó≠ã {{ .StashCount }}{{ end }} ",
                    "trailing_diamond": "ÓÇ¥",
                    "type": "git"
                },
                {
                    "background": "#6CA35E",
                    "foreground": "#ffffff",
                    "powerline_symbol": "ÓÇ∞",
                    "properties": {
                        "fetch_version": true
                    },
                    "style": "powerline",
                    "template": " Óúò {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
                    "type": "node"
                },
                {
                    "background": "#8ED1F7",
                    "foreground": "#111111",
                    "powerline_symbol": "ÓÇ∞",
                    "properties": {
                        "fetch_version": true
                    },
                    "style": "powerline",
                    "template": " Óò¶ {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
                    "type": "go"
                },
                {
                    "background": "#4063D8",
                    "foreground": "#111111",
                    "powerline_symbol": "ÓÇ∞",
                    "properties": {
                        "fetch_version": true
                    },
                    "style": "powerline",
                    "template": " Óò§ {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
                    "type": "julia"
                },
                {
                    "background": "#FFDE57",
                    "foreground": "#111111",
                    "powerline_symbol": "ÓÇ∞",
                    "properties": {
                        "display_mode": "files",
                        "fetch_virtual_env": true
                    },
                    "style": "powerline",
                    "template": "{{ if .Venv }}üêç {{ .Venv }} | {{ end }}Óàµ {{ .Full }}",
                    "type": "python"
                },
                {
                    "background": "#AE1401",
                    "foreground": "#ffffff",
                    "powerline_symbol": "ÓÇ∞",
                    "properties": {
                        "display_mode": "files",
                        "fetch_version": true
                    },
                    "style": "powerline",
                    "template": " Óûë {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
                    "type": "ruby"
                },
                {
                    "background": "#FEAC19",
                    "foreground": "#ffffff",
                    "powerline_symbol": "ÓÇ∞",
                    "properties": {
                        "display_mode": "files",
                        "fetch_version": false
                    },
                    "style": "powerline",
                    "template": " ÔÉß{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
                    "type": "azfunc"
                },
                {
                    "background_templates": [
                        "{{if contains \"default\" .Profile}}#FFA400{{end}}",
                        "{{if contains \"jan\" .Profile}}#f1184c{{end}}"
                    ],
                    "foreground": "#ffffff",
                    "powerline_symbol": "ÓÇ∞",
                    "properties": {
                        "display_default": false
                    },
                    "style": "powerline",
                    "template": " Óû≠ {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} ",
                    "type": "aws"
                },
                {
                    "background": "#ffff66",
                    "foreground": "#111111",
                    "powerline_symbol": "ÓÇ∞",
                    "style": "powerline",
                    "template": " ÔÇ≠ ",
                    "type": "root"
                },
                {
                    "background": "#503e2c",
                    "foreground": "#111111",
                    "powerline_symbol": "ÓÇ∞",
                    "style": "plain",
                    "template": "<transparent>ÓÇ∞</> üïí {{ .CurrentDate | date .Format }} ",
                    "type": "time"
                },
                {
                    "background": "#31b166",
                    "foreground": "#ffffff",
                    "powerline_symbol": "ÓÇ∞",
                    "properties": {
                        "charged_icon": "üîã ",
                        "charging_icon": "‚ö° ",
                        "discharging_icon": "üîã "
                    },
                    "style": "diamond",
                    "template": "<parentBackground>ÓÇ∞</> {{ .Icon }}{{ .Percentage }}% ",
                    "trailing_diamond": "ÓÇ∞",
                    "type": "battery"
                },
                {
                    "background": "#83769c",
                    "foreground": "#ffffff",
                    "properties": {
                        "always_enabled": true
                    },
                    "style": "plain",
                    "template": "<transparent>ÓÇ∞</> ÓÆ¢ {{ .FormattedMs }} ",
                    "type": "executiontime"
                },
                {
                    "background": "#00897b",
                    "background_templates": ["{{ if gt .Code 0 }}#e91e63{{ end }}"],
                    "foreground": "#ffffff",
                    "properties": {
                        "always_enabled": true
                    },
                    "style": "diamond",
                    "template": "<parentBackground>ÓÇ∞</> Óà∫ ",
                    "trailing_diamond": "ÓÇ¥",
                    "type": "status"
                }
            ],
            "type": "prompt"
        },
        {
            "segments": [
                {
                    "background": "#0077c2",
                    "foreground": "#ffffff",
                    "style": "plain",
                    "template": "<#0077c2,transparent>ÓÇ∂</> Ôíâ {{ .Name }} <transparent,#0077c2>ÓÇ≤</>",
                    "type": "shell"
                },
                {
                    "background": "#1BD760",
                    "foreground": "#111111",
                    "invert_powerline": true,
                    "powerline_symbol": "ÓÇ≤",
                    "properties": {
                        "paused_icon": "ÔÅå ",
                        "playing_icon": "ÔÅã "
                    },
                    "style": "powerline",
                    "template": " ÔÖß {{ .Icon }}{{ if ne .Status \"stopped\" }}{{ .Artist }} - {{ .Track }}{{ end }} ",
                    "type": "ytm"
                },
                {
                    "background": "#f36943",
                    "background_templates": [
                        "{{if eq \"Charging\" .State.String}}#40c4ff{{end}}",
                        "{{if eq \"Discharging\" .State.String}}#ff5722{{end}}",
                        "{{if eq \"Full\" .State.String}}#4caf50{{end}}"
                    ],
                    "foreground": "#ffffff",
                    "invert_powerline": true,
                    "powerline_symbol": "ÓÇ≤",
                    "properties": {
                        "charged_icon": "ÓàØ ",
                        "charging_icon": "Óà¥ ",
                        "discharging_icon": "Óà± "
                    },
                    "style": "powerline",
                    "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}Ôäï ",
                    "type": "battery"
                },
                {
                    "background": "#2e9599",
                    "foreground": "#111111",
                    "invert_powerline": true,
                    "leading_diamond": "ÓÇ≤",
                    "style": "diamond",
                    "template": " {{ .CurrentDate | date .Format }} ",
                    "trailing_diamond": "ÓÇ¥",
                    "type": "time"
                }
            ],
            "type": "rprompt"
        }
    ],
    "console_title_template": "{{ .Shell }} in {{ .Folder }}",
    "final_space": true,
    "version": 3
}
